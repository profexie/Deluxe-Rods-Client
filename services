        <script>
            function tccl_dpsid() {
                for (var t = "dps_site_id=", i = document.cookie.split(";"), e = 0; e < i.length; e++) {
                    for (var n = i[e]; " " == n.charAt(0); )
                        n = n.substring(1);
                    if (0 == n.indexOf(t))
                        return n.substring(t.length, n.length)
                }
                return null
            }
